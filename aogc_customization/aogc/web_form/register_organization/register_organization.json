 "client_script": "$('.discard-btn').removeClass('btn-default').addClass('btn-danger');\nfrappe.web_form.on('license_issue_date', (field, value) => {\n    var today = frappe.datetime.get_today();\n\n    if (value && value > today) {\n        frappe.msgprint('You cannot select a future date in License issue date');\n        frappe.web_form.set_value('license_issue_date', '');\n    }\n});\n\nfrappe.web_form.on('license_expiry_date', (field, value) => {\n    var today = frappe.datetime.get_today();\n\n    if (value && value < today) {\n        frappe.msgprint('License has already expired');\n        frappe.web_form.set_value('license_expiry_date', '');\n    }\n});\n\nfrappe.web_form.on('establish_date', (field, value) => {\n    var today = frappe.datetime.get_today();\n\n    if (value && value > today) {\n        frappe.msgprint('Establishment date cannot be a future date');\n        frappe.web_form.set_value('establish_date', '');\n    }\n});\n\nfrappe.web_form.on('tax_clearance_date', (field, value) => {\n    var today = frappe.datetime.get_today();\n\n    if (value && value < today) {\n        frappe.msgprint('Tax clearance date cannot be in the past');\n        frappe.web_form.set_value('tax_clearance_date', '');\n    }\n});\n\n// Additional validation for license_expiry_date\nfrappe.web_form.on('license_expiry_date', (field, value) => {\n    var today = frappe.datetime.get_today();\n\n    if (value && value < today) {\n        frappe.msgprint('License expiry date cannot be in the past');\n        frappe.web_form.set_value('license_expiry_date', '');\n    }\n});\n",
